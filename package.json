{
  "name": "grud-aggregator",
  "version": "9.0.0",
  "description": "Core aggregator functions",
  "files": [
    "dist"
  ],
  "bin": {
    "grud-aggregator": "./src/structure/init.js"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./entities": {
      "types": "./dist/entities.d.ts",
      "import": "./dist/entities.js",
      "require": "./dist/entities.cjs"
    },
    "./referencer": {
      "types": "./dist/referencer/referencer.d.ts",
      "import": "./dist/referencer/referencer.js",
      "require": "./dist/referencer/referencer.cjs"
    }
  },
  "scripts": {
    "build": "tsdown",
    "prepublish": "npm run build && npm run lint && npm run test && npm run test:dts",
    "coverage": "nyc npm run test",
    "lint": "eslint src/",
    "lint:watch": "eslint src/ -w",
    "test": "mocha --colors $(find . -type d -name node_modules -prune -o -name '*.spec.js')",
    "test:dts": "vitest run ./src/index.spec-d.ts",
    "test:watch": "npm run test -- -w"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:campudus/grud-aggregator.git"
  },
  "keywords": [
    "aggregate",
    "aggregation",
    "data",
    "grud"
  ],
  "author": "Campudus GmbH",
  "license": "Apache-2.0",
  "homepage": "https://github.com/campudus/grud-aggregator#readme",
  "devDependencies": {
    "@types/node": "24.7.2",
    "@typescript-eslint/eslint-plugin": "8.46.1",
    "@typescript-eslint/parser": "8.46.1",
    "eslint": "8.57.1",
    "express": "4.14.0",
    "mocha": "11.7.4",
    "must": "0.13.1",
    "nyc": "17.1.0",
    "prettier": "3.6.2",
    "tmp": "0.0.28",
    "tsdown": "^0.15.7",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "fs-extra": "0.30.0",
    "lodash": "4.13.1",
    "lowdb": "0.13.1",
    "sharp": "0.31.1"
  }
}